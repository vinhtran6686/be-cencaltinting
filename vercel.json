{
  "version": 2,
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "framework": "nestjs",
  "devCommand": "yarn dev",
  "installCommand": "yarn install",
  "builds": [
    {
      "src": "dist/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js"
    }
  ]
}
